# AUTOGENERATED FOR {{ game_name }} - MODIFICATIONS WILL BE LOST
[package]
name = "{{ package }}"
version = "0.1.0"
edition = "2021"

[lib]
name = "lib{{ package }}"
path = "src/lib.rs"

[[bin]]
name = "{{ package }}"
path = "bin/main.rs"

[dependencies]
analyzer = { path = "../../analyzer" }
bitflags = { version = "2.*", features = ["serde"] }
enum-map = { version = "2.5", features = ["serde"] }
serde = { version = "1.0", features = ["derive"] }
yaml-rust = "*"

[dev-dependencies]
criterion = "*"

[[bench]]
name = "bench"
harness = false

[profile.release]
debug = true