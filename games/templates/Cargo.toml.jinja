# AUTOGENERATED FOR {{ game_name }} - MODIFICATIONS WILL BE LOST
[package]
name = "{{ package }}"
version = "0.1.0"
edition = "2021"

[lib]
name = "lib{{ package }}"
path = "src/lib.rs"

[[bin]]
name = "{{ package }}"
path = "bin/main.rs"

[dependencies]
analyzer = { path = "../../analyzer" }
bitflags = { version = "2.*", features = ["serde"] }
clap = { version = "4.*", features = ["derive"] }
enum-map = { version = "2.5", features = ["serde"] }
serde = { version = "1.0", features = ["derive"] }
serde_repr = "*"
yaml-rust = "*"
zstd = "*"

[dev-dependencies]
criterion = "*"
rmp-serde = "*"
serde_json = "*"

[[bench]]
name = "bench"
harness = false

[profile.dev]
overflow-checks = false

[profile.release]
debug = true
lto = "fat"
panic = "abort"

[[test]]
name = "unittest"
path = "tests/unittest.rs"
harness = false